{
  "name": "telegraf-ecosystem",
  "scripts": {
    "dev": "nodemon -q",
    "build": "rimraf dist && tsc --project tsconfig.build.json && rimraf tsconfig.build.tsbuildinfo",
    "publish-to-npm": "npm run build && npm version patch && npm publish",
    "dev:officer-bot": "nodemon -q --config nodemon.common.json",
    "fast-save": "git add . && git commit -m \"fast-save\" && git push && npm run publish-to-npm"
  },
  "license": "MIT",
  "author": "Safronov Maxim (impuLssse911)",
  "version": "0.0.34",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "tsconfig.json",
    "tsconfig.build.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/impuLssse/telegraf-decorators.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@telegraf/types": "^7.1.0",
    "awilix": "^12.0.3",
    "awilix-manager": "^5.5.0",
    "dotenv": "^16.4.5",
    "telegram-keyboard": "^2.3.5",
    "winston": "^3.17.0"
  },
  "peerDependencies": {
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "chalk": "^5.3.0",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0"
  },
  "keywords": [
    "telegraf",
    "telegram",
    "telegram bot api",
    "bot",
    "botapi",
    "bot framework",
    "telegram-eco",
    "telegram-ecosystem"
  ]
}
